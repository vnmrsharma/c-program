/* Take two input,
   perform mathematical operations 
   and print result*/

#include<stdio.h>

int main (){

    //if input is integer value
    int num1, num2, result;
    printf("Enter two integer values: \n");
    scanf("%d %d", &num1, &num2);
    result = num1  - num2;
    printf("%d - %d = %d \n", num1,num2, result);

    return 0;
}
